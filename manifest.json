{
	"manifest_version": 2,
	"name": "Update Cococ",
	"version": "1.0",
	"description": "Update new feature",
	"icon": {
		"128": "icon128.png",
		"48": "icon48.png",
		"16": "icon16.png"
	},
	"browser_action":{
		"default_icon": "icon16.png",
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"background": {
		"scripts": ["jquery-3.3.1.min.js", "eventPage.js"],
		"persistent": true
	},
	"content_scripts": [
		{
			"matches": ["https://www.google.com/*"],
			"js": ["jquery-3.3.1.min.js", "content.js"]
		}
	],
	"permissions": [
		"storage",
		"notifications",
		"https://www.google.com/*",
		"http://google-search.esy.es/*"

	]
}